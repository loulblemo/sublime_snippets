${1:import glob
import os
import music21}

files = [f for f in glob.glob(os.path.join(${2:folder}, '*.${3:mid}')) if os.path.isfile(f)]

for f in files:
    score = music21.converter.parse(f)

    for ${5:element} in score.${4:elements}:
        print ${5:element}

